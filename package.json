{
  "name": "exchange-rates",
  "version": "1.0.0",
  "description": "Exchange rates frontend",
  "main": "index.js",
  "scripts": {
    "dev": " npm-run-all --parallel server-watch watch",
    "server-watch": "nodemon --watch src/server index.js",
    "build": "webpack --config webpack.babel.js",
    "start": "node index.js",
    "watch": "webpack-dev-server --config webpack.dev.babel.js --inline",
    "test": "tape -r babel-core/register src/**/*.spec.js*",
    "lint": "eslint src --ext .js --ext .jsx",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "postcss-loader": "^2.0.5",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react-hot-loader": "^1.3.1",
    "tape": "^4.6.3",
    "webpack-dev-server": "^2.4.5",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
      "window": true,
      "fetch": true
    },
    "rules": {
      "react/forbid-prop-types": 0,
      "import/no-named-as-default": 0,
      "import/no-extraneous-dependencies": 0
    }
  }
}
